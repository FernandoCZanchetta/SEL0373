{
  "name": "site",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production --base-href https://fernandoczanchetta.github.io/SEL0373/",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "npx eslint . && npx prettier -c .",
    "format": "npx eslint --fix .; npx prettier --write .",
    "precommit": "npm lint-staged && npm lint"
  },
  "lint-staged": {
    "*.{ts,js,json,scss,sass,css,md}": [
      "npx eslint --fix .",
      "npx prettier --write .",
      "git add"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.1.0",
    "@angular/common": "~13.1.0",
    "@angular/compiler": "~13.1.0",
    "@angular/core": "~13.1.0",
    "@angular/forms": "~13.1.0",
    "@angular/platform-browser": "~13.1.0",
    "@angular/platform-browser-dynamic": "~13.1.0",
    "@angular/router": "~13.1.0",
    "bootstrap": "^5.1.3",
    "ngx-bootstrap": "^8.0.0",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.1.3",
    "@angular/cli": "~13.1.3",
    "@angular/compiler-cli": "~13.1.0",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.5.0",
    "@types/bootstrap": "^5.1.9",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.20.43",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "eslint": "^8.7.0",
    "globals": "^15.6.0",
    "husky": "^7.0.4",
    "jasmine-core": "~3.10.0",
    "lint-staged": "^12.2.0",
    "prettier": "^2.5.1",
    "typescript": "~4.5.2"
  }
}
